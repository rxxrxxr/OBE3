<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.onezogreen.onezo3.sales.SalesMapper">
    <select id="getSalesByStore" resultType="SaleVo">
        SELECT * FROM sales WHERE store_id = #{storeId}
    </select>

    <select id="getSalesByMenu" resultType="SaleVo">
        SELECT * FROM sales WHERE store_id = #{storeId} AND menu_id = #{menuId}
    </select>
</mapper>
